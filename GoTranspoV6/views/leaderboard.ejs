<% include ./partials/header %>


<style>


</style>

<div class="text-center lead">Leaderboard</div>

<div class="container">

<table class="table">
        <thead class="thead-dark">
          <tr>
            <th scope="col">#</th>
            <th scope="col">User Name</th>
            <th scope="col">Coins</th>
          </tr>
        </thead>
        <tbody>

            <% for(var i=0; i< users.length; i++){ %>

                <tr>
                        <th scope="row"><%= i+1 %></th>
                        <td><%= users[i].username %></td>
                        <td><%= users[i].coins %></td>
                      </tr>

            <% } %>
        </tbody>
      </table>

    </div>


